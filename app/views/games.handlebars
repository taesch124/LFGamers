<div class="row">
  
  <div class="col s12">
    <h3>Game Results</h3>
    {{#each games }}
    <div class="card" data-id="{{id}}">

      <div class="row flex">
        <div class="col s4">
          <img src="http:{{poster}}" alt="{{name}}'s poster'">
        </div>
        <div class="col s8">
          <h4><strong>{{name}}</strong></h4>
          <p>Popularity: {{popularity}}%</p>
        </div>
      </div>

    </div>
    {{/each}}
  </div>

</div>